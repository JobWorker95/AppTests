<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.valer.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity"
            android:icon="@drawable/ask"
            android:label="Тест"
            android:theme="@style/Theme.AppCompat"></activity>
        <activity android:name=".FirstActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TeacherActivity"
                  android:label="Главное Меню"
                  android:theme="@style/Theme.AppCompat"></activity>

        <activity android:name=".StudentActivity" />

        <activity android:name=".CreateTestActivity"
                  android:label="Создать Тест"
                  android:theme="@style/Theme.AppCompat"></activity>

        <activity android:name=".CheckActivity"></activity>
    </application>

</manifest>